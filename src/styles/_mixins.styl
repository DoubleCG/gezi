clx()
  zoom: 1;
  &:after
    clear: both;
    content: ".";
    display: block;
    height: 0;
    visibility: hidden;
    overflow: hidden;

ama($style = all, $s = .3s, $amaType = ease, $delay = 0s)
  -webkit-transition: $style $s $amaType $delay;
  -moz-transition: $style $s $amaType $delay;
  -ms-transition: $style $s $amaType $delay;
  -o-transition: $style $s $amaType $delay;
  transition: $style $s $amaType $delay;

transform($rotateX=0deg, $rotateY = 0deg, $translateZ= 0px)
  -webkit-transform: rotateX($rotateX) rotateY($rotateY) translateZ($translateZ);
  -moz-transform: rotateX($rotateX) rotateY($rotateY) translateZ($translateZ);
  -o-transform: rotateX($rotateX) rotateY($rotateY) translateZ($translateZ);
  -ms-transform: rotateX($rotateX) rotateY($rotateY) translateZ($translateZ);
  transform: rotateX($rotateX) rotateY($rotateY) translateZ($translateZ);

