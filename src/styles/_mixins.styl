demo-pose-cube()
  -webkit-transform-style: preserve-3d;
  -moz-transform-style: preserve-3d;
  -o-transform-style: preserve-3d;
  -ms-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-transition: -webkit-transform linear .2s;
  -moz-transition: -moz-transform linear .2s;
  -o-transition: -o-transform linear .2s;
  -ms-transition: -ms-transform linear .2s;
  transition: transform linear .2s;
  -webkit-transform: rotateX(-15deg) rotateZ(0deg) rotateY(-30deg);
  -moz-transform: rotateX(-15deg) rotateZ(0deg) rotateY(-30deg);
  -o-transform: rotateX(-15deg) rotateZ(0deg) rotateY(-30deg);
  -ms-transform: rotateX(-15deg) rotateZ(0deg) rotateY(-30deg);
  transform: rotateX(-15deg) rotateZ(0deg) rotateY(-30deg);
  -webkit-transform-origin: 20px 20px;
  -moz-transform-origin: 20px 20px;
  -o-transform-origin: 20px 20px;
  -ms-transform-origin: 20px 20px;
  transform-origin: 20px 20px

